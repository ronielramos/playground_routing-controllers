{
   "name": "playground_routing-controllers",
   "version": "0.1.0",
   "description": "test project of routing-controllers and typeorm together",
   "main": "src/app.ts",
   "scripts": {
      "lint": "eslint src/** --ext .ts",
      "build": "tsc",
      "start": "nodemon src/server.ts",
      "m:create": "node -r dotenv/config -r ts-node/register ./node_modules/typeorm/cli.js migration:create -n",
      "m:up": "node -r dotenv/config -r ts-node/register ./node_modules/typeorm/cli.js migration:run",
      "m:down": "node -r dotenv/config -r ts-node/register ./node_modules/typeorm/cli.js migration:revert",
      "m:generate": "node -r dotenv/config -r ts-node/register ./node_modules/typeorm/cli.js migration:generate -n",
      "test": "echo \"Error: no test specified\" && exit 1"
   },
   "author": "Roniel Ramos Gomes",
   "license": "ISC",
   "dependencies": {
      "body-parser": "^1.19.0",
      "class-transformer": "^0.2.3",
      "class-validator": "^0.11.0",
      "compression": "^1.7.4",
      "cors": "^2.8.5",
      "dotenv": "^8.2.0",
      "express": "^4.17.1",
      "helmet": "^3.21.3",
      "jest": "^25.1.0",
      "multer": "^1.4.2",
      "mysql2": "^2.1.0",
      "reflect-metadata": "^0.1.10",
      "routing-controllers": "^0.8.1-alpha.2",
      "ts-jest": "^25.2.1",
      "typedi": "^0.8.0",
      "typeorm": "0.2.25",
      "typescript": "^3.8.3"
   },
   "devDependencies": {
      "@types/body-parser": "^1.19.0",
      "@types/compression": "^1.7.0",
      "@types/cors": "^2.8.6",
      "@types/dotenv": "^8.2.0",
      "@types/express": "^4.17.3",
      "@types/helmet": "0.0.45",
      "@types/jest": "^25.1.4",
      "@types/multer": "^1.4.2",
      "@types/node": "^8.0.29",
      "@typescript-eslint/eslint-plugin": "^2.22.0",
      "@typescript-eslint/parser": "^2.22.0",
      "eslint": "^6.8.0",
      "eslint-config-standard": "^14.1.0",
      "eslint-plugin-import": "^2.20.1",
      "eslint-plugin-node": "^11.0.0",
      "eslint-plugin-promise": "^4.2.1",
      "eslint-plugin-standard": "^4.0.1",
      "nodemon": "^2.0.2",
      "ts-node": "3.3.0",
      "tsconfig-paths": "^3.9.0"
   }
}
